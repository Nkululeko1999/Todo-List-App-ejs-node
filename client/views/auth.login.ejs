<%- include("header.ejs")%>
    <form class="form-signin">
      <h1 class="h3 mb-3 font-weight-normal">Please sign in</h1>
      <label for="inputEmail" class="sr-only mt-3">Email address</label>
      <input type="email" id="inputEmail" class="form-control mt-3 p-2" placeholder="Email address" required="" autofocus="">
      <label for="inputPassword" class="sr-only mt-3">Password</label>
      <input type="password" id="inputPassword" class="form-control mt-3 p-2" placeholder="Password" required="">
      <div class="mb-3">
        <p class="mt-3">Don't have an account 
          <a class="auth-link" href="">Sign Up</a>
        </p>
      </div>
      <button class="auth-btn btn-primary btn-block p-2" type="submit">Sign In</button>
    </form>
<%- include("footer.ejs")%>