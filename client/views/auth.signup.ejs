<%- include("header.ejs")%>
    <form class="form-signin">
      <h1 class="h3 mb-3 font-weight-normal">Please sign up</h1>
      <label for="inputEmail" class="sr-only">Email address</label>
      <input type="email" id="inputEmail" class="form-control mt-3 p-2" placeholder="Email address" required="" autofocus="">
      <label for="inputPassword" class="sr-only">Password</label>
      <input type="password" id="inputPassword" class="form-control mt-3 p-2" placeholder="Password" required="">
      <button class="auth-btn btn-primary btn-block mt-3 p-2" type="submit">Sign up</button>
      <div class="mb-3 mt-3">
        <p class="mt-3">Have an account 
          <a class="auth-link" href="">Login</a>
        </p>
      </div>
    </form>
<%- include("footer.ejs")%>